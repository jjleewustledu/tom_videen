desc
@X window version, first release.
@


1.1
log
@Initial revision
@
text
@C$Id$
C$Log$
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
C  Subroutine:   VTABLE.FTN
C
C  Author:       Tom O. Videen
C  Date:         20-Jul-90
C  Written For:  RAPTURE
C
C  History:
C     Modified 17-Nov-93 by Tom Yang to port to UNIX workstations.
C
C  Description:
C     This subroutine returns critical Mann-Whitney V for 2-tailed
C     significance for n,m.
C     If no critical value, VAL = 9999.
C     If n or m > 20, VAL = -9999.
C
C     Table entered from Table B of Spitznagel, p.199
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
      SUBROUTINE VTABLE(VCRIT,M,N)
C
      REAL*4     VCRIT    ! critical V
      INTEGER*4  M        ! no. in first set
      INTEGER*4  N        ! no. in second set
      CHARACTER*256 RCSHEADER
C
      REAL*4     C(20,20) ! table of critical values of v for
C                           M=1,20 and N=1,20
C
C
      DATA (C(1,I),I=1,20)
     &     /20*9999./
      DATA (C(2,I),I=1,20)
     &     /9999.,9999.,9999.,9999.,9999.,9999.,9999.,8.0,9.0,10.0,
     &      11.0,11.0,12.0,13.0,14.0,15.0,15.0,16.0,17.0,18.0/
      DATA (C(3,I),I=1,20)
     &     /9999.,9999.,9999.,9999.,7.5,8.0,9.5,10.0,11.5,12.0,
     &      13.5,14.0,15.5,16.0,17.5,18.0,19.5,20.0,21.5,22.0/
      DATA (C(4,I),I=1,20)
     &     /9999.,9999.,9999.,8.0,9.0,10.0,11.0,12.0,14.0,15.0,
     &      16.0,17.0,18.0,19.0,20.0,21.0,23.0,24.0,25.0,26.0/
      DATA (C(5,I),I=1,20)
     &     /9999.,9999.,7.5,9.0,10.5,12.0,12.5,14.0,15.5,17.0,
     &      18.5,19.5,20.5,22.0,23.5,25.0,25.5,27.0,28.5,30.0/
C
      DATA (C(6,I),I=1,20)
     &     /9999.,9999.,8.0,10.0,12.0,13.0,15.0,16.0,17.0,19.0,
     &      20.0,22.0,23.0,25.0,26.0,27.0,29.0,30.0,32.0,33.0/
      DATA (C(7,I),I=1,20)
     &     /9999.,9999.,9.5,11.0,12.5,15.0,16.5,18.0,19.5,21.0,
     &      22.5,24.0,25.5,27.0,28.5,30.0,31.5,33.0,34.5,36.0/
      DATA (C(8,I),I=1,20)
     &     /9999.,8.0,10.0,12.0,14.0,16.0,18.0,19.0,21.0,23.0,
     &      25.0,26.0,28.0,30.0,31.0,33.0,34.0,36.0,38.0,39.0/
      DATA (C(9,I),I=1,20)
     &     /9999.,9.0,11.5,14.0,15.5,17.0,19.5,21.0,23.5,25.5,
     &      26.5,28.0,30.5,32.0,33.5,35.0,37.5,39.0,40.5,42.0/
      DATA (C(10,I),I=1,20)
     &     /9999.,10.0,12.0,15.0,17.0,19.0,21.0,23.0,25.0,27.0,
     &      29.0,31.0,32.0,34.0,36.0,38.0,40.0,42.0,43.0,45.0/
C
      DATA (C(11,I),I=1,20)
     &     /9999.,11.0,13.5,16.0,18.5,20.0,22.5,25.0,26.5,29.0,
     &      30.5,33.0,34.5,37.0,38.0,41.0,42.5,44.0,46.5,48.0/
      DATA (C(12,I),I=1,20)
     &     /9999.,11.0,14.0,17.0,19.0,22.0,24.0,26.0,28.0,31.0,
     &      33.0,35.0,37.0,39.0,41.0,43.0,45.0,47.0,49.0,51.0/
      DATA (C(13,I),I=1,20)
     &     /9999.,12.0,15.5,18.0,20.5,23.0,25.5,28.0,30.5,32.5,
     &      34.5,37.0,39.5,41.0,43.5,45.0,47.5,50.0,51.5,54.0/
      DATA (C(14,I),I=1,20)
     &     /9999.,13.0,16.0,19.0,22.0,25.0,27.0,30.0,32.0,34.0,
     &      37.0,39.0,41.0,43.0,46.0,48.0,50.0,52.0,55.0,57.0/
      DATA (C(15,I),I=1,20)
     &     /9999.,14.0,17.5,20.0,23.5,26.0,28.5,31.0,33.5,36.0,
     &      38.5,41.0,43.5,46.0,48.5,50.0,52.5,55.0,57.5,60.0/
C
      DATA (C(16,I),I=1,20)
     &     /9999.,15.0,18.0,21.0,25.0,-27.0,30.0,33.0,35.0,38.0,
     &      41.0,43.0,45.0,48.0,50.0,53.0,55.0,58.0,60.0,62.0/
      DATA (C(17,I),I=1,20)
     &     /9999.,15.0,19.5,23.0,25.5,29.0,31.5,34.0,37.5,40.0,
     &      42.5,45.0,47.5,50.0,52.5,55.0,57.5,60.0,62.5,65.0/
      DATA (C(18,I),I=1,20)
     &     /9999.,16.0,20.0,24.0,27.0,30.0,33.0,36.0,39.0,42.0,
     &      44.0,47.0,50.0,52.0,55.0,58.0,60.0,63.0,65.0,68.0/
      DATA (C(19,I),I=1,20)
     &     /9999.,17.0,21.5,25.0,28.5,32.0,34.5,38.0,40.5,43.0,
     &      46.5,49.0,51.5,55.0,57.5,60.0,62.5,65.0,67.5,71.0/
      DATA (C(20,I),I=1,20)
     &     /9999.,18.0,22.0,26.0,30.0,33.0,36.0,39.0,42.0,45.0,
     &      48.0,51.0,54.0,57.0,60.0,62.0,65.0,68.0,71.0,73.0/
C
      RCSHEADER = '$Id$'
      IF (M.GT.20 .OR. N.GT.20) THEN
        VCRIT = -9999.
      ELSE
        VCRIT = C(M,N)
      END IF
C
      RETURN
      END
@
