@/*$Id$*/
/*$Log$*/

#include <time.h>

static char rcsid [] = "$Header$";

void	intdate_ (datearray)
long	datearray [];
{
	struct tm	*t;
	long		l;

	l	= time ((long *) 0);
	t	= localtime (&l);
	datearray [0]	= t->tm_year;
	datearray [1]	= t->tm_mon + 1;
	datearray [2]	= t->tm_mday;
}
@
