#ifndef sun

#ifndef sincos_h
#define sincos_h
#include <math.h>

static void sincos(theta, sintheta, costheta)
double theta, *sintheta, *costheta;
{
	double tmp = sin(theta);
	*sintheta = tmp;
	*costheta = sqrt(1 - tmp * tmp);
}
#endif /* sincos_h */

#endif /* sun */
