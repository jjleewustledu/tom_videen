*******************************************************
*  TISCRV.LNK                         {TOV  04-JUN-92}
*  Link modules for TISCRV.TSK
*******************************************************
XDEL LINK.CMD
XAL LINK.MAP,IN
LO LINK,30
$BUILD LINK.CMD
TITLE 'TASK BUILD OF TISCRV.TSK'
ES TA
OPTION FL,DFL,NAFP,NKEY,ROLL,XSVC1
OPTION SYS=70000,WORK=(30000,512000)
OPTION IOB=20,VFC
INCL TISCRV
INCL RDATA
INCL SYS:GETIMAGE.OBJ/S
LI SYS0:TERMINAL.LIB/S
LI SYS0:F7RTL55.OBJ/S
MAP LINK.MAP,ADDR
BUILD TISCRV.TSK
END
$ENDB
XDEL TISCRV.TSK
T LINK
ST ,C=LINK.CMD,L=LINK.MAP
$IFNE 0
  $WR ***@0: LINK ENDED ABNORMALLY
  $WR ***@0: SEE LINK.MAP
  $CLEAR
$ELSE
  $WR <<<  TASK BUILD OF TISCRV.TSK SUCCESSFUL   >>>
  XDEL LINK.CMD
  XDEL LINK.MAP
$ENDC
$EXIT
