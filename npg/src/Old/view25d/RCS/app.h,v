typedef struct 
{
	DRAW_DATA	*draw_data;
	char		node_flag;		/* 0: redraw; 1: select; 2: cancel. */
	Widget		dialog;
	PARAMETERS_DATA	*default_parameters;
} DATA_TYPE_DATA;

typedef struct 
{
	Display		*display;
	Window		root_window;
