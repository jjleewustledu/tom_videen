	/* 
	 * Retrieve the colors of the widget.  
	 */
	n = 0;
	XtSetArg (arglist [n], XtNforeground, &foreground); n++;
	XtGetValues (toggleAF3DSymbol [0], arglist, n);

	/* 
	 * Get a tile corresponding the given pattern.  
	 * Display the rectangle pixmap in the button and also store it. 
	 */
	for (i = 0; i < MAX_SYMBOL_NUM; i++)
	{
		AF3DSymbols [i] = XmGetPixmap (XtScreen (toggleAF3DSymbol [i]),  
					symbol_names[i], foreground, background);
		n = 0;
		XtSetArg (arglist [n], XmNlabelType, XmPIXMAP); n++;
		XtSetArg (arglist [n], XmNlabelPixmap, AF3DSymbols [i]); n++;
		XtSetValues (toggleAF3DSymbol [i], arglist, n);
	}

	n = 0;
	XtSetArg (arglist[n], XtNforeground, &foreground); n++;
	XtGetValues (toggleAF3DColor [*symbol_color_code], arglist, n);
