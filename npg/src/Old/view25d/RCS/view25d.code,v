	toggleAF3DSymbol [0] = XtCreateManagedWidget ("toggleAF3DSymbol0", 
						xmToggleButtonWidgetClass, browser, NULL, 0);
	XtAddCallback (toggleAF3DSymbol [0], XmNvalueChangedCallback, SetRadioToggleCallback, NULL);

	for (i = 1; i < MAX_SYMBOL_NUM; i++)
	{
		toggleAF3DSymbol [i] = XtCreateManagedWidget ("toggleAF3DSymbol", 
					xmToggleButtonWidgetClass, browser, NULL, 0);
	}

	n = 0;
	XtSetArg (arglist[n], XmNmarginLeft, 25); n++;
	XtSetArg (arglist[n], XmNshadowThickness, 0); n++;
	XtSetArg (arglist[n], XmNset, TRUE); n++;
	XtSetArg (arglist[n], XmNtopAttachment, XmATTACH_WIDGET); n++;
