	}

	/* 
	 * Retrieve the colors of the widget.  
	 */
	n = 0;
	XtSetArg (arglist [n], XtNbackground, &background); n++;
	XtSetArg (arglist [n], XtNforeground, &foreground); n++;
	XtGetValues (toggleRegionPattern [0], arglist, n);

	/* 
	 * Get a tile corresponding the given pattern.  
	 * Display the rectangle pixmap in the button and also store it. 
	 */
	for (i = 0; i < REGION_STYLE_NUM; i++)
	{
		RegionStyles [i] = XmGetPixmap (XtScreen (toggleRegionPattern [i]),  
					symbol_names [*(region_type + i)], foreground, background);
		n = 0;
		XtSetArg (arglist [n], XmNlabelType, XmPIXMAP); n++;
		XtSetArg (arglist [n], XmNlabelPixmap, RegionStyles [i]); n++;
		XtSetValues (toggleRegionPattern [i], arglist, n);
	}

	n = 0;
