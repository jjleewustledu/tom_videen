	$Id */
/*  ========================================================================
 *  Module:         vecprint.c
 *  Date:           Jun 1995
 *  Author:         Tom Videen
 *  Description:    Print vector a to stdout.
 *  Input:
 *		str		-	string label for vector
 *      vec[n][m] -   vector
 *  ========================================================================
 */
/*@@unused@@*/
static char     rcsid[] = "$Header: /home/npggw/tom/src/matdir/RCS/vecprint.c,v 1.1 1995/10/18 19:11:58 tom Exp tom $";

#include <stdio.h>

void            vecprint (str, vec, n)
	char           *str;
	float          *vec;
	int             n;
{
	int             i;

	fprintf (stdout, "%s  ", str);
	for (i = 1; i < n; i++)
		fprintf (stdout, "%10.5f  ", vec[i]);
	fprintf (stdout, "\n");
}
@


