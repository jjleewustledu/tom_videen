@#!/bin/csh
#	Name:		scanhdr
#	Date:		21-Sep-93
#	Author:		Tom Videen
#	Function:	Get hdr information on an ECAT raw scan file

set argc=$#argv
if ($argc != 1) then
  echo "Usage: scanhdr filename"
  exit
endif

/usr/local/bin/scanhdr.bin $1
@
