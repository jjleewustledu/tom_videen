*  LINKOPR.CSS                  (TOV 15-Jun-87)
*  Link OPROFILE.TSK
************************************************
XDEL LINK.CMD
LO LINK,30
$BUILD LINK.CMD
TITLE 'TASK BUILD OF OPROFILE.TSK'
ES TA
OPTION FL,DFL,NAFP,NKEY,ROLL,XSVC1
OPTION SYS=20000,WORK=(15000,256000)
OPTION IOB=20,VFC
INCL OPROFILE
INCL OAXES
INCL OFILE
INCL OPLOT
INCL OPLOTDAT
INCL OPLOTYPE
INCL BETAI
INCL RTNI
INCL QSF
INCL FCTOFP
INCL BETADENS
INCL POFSIGMA
INCL SIGMAOFP
INCL GAMMA
INCL EXPTRAP
LI SYS:TERMINAL.LIB/S
LI SYS:GRAPHON.LIB/S
LI SYS:F7RTL.OBJ/S
MAP @1,ADDR
BUILD OPROFILE.TSK
END
$ENDB
XDEL OPROFILE.TSK
T LINK
ST ,C=LINK.CMD,L=@1
$IFNE 0
  $WR ***@0: LINK ENDED ABNORMALLY
  $CLEAR
$ELSE
  $WR <<<  TASK BUILD OF OPROFILE.TSK SUCCESSFUL   >>>
$ENDC
XDE LINK.CMD
$EXIT
