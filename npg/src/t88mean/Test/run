echo PETT VI SINGLE > log1
t88mean prm1s lst1 org1s.t88 >> log1
t88 prm1s lst1 new1s.t88 >> log1
echo PETT VI PAIRED >> log1
t88mean prm1p lst1 org1p.t88 >> log1
t88 prm1p lst1 new1p.t88 >> log1

echo ECAT 6 SINGLE > log2
t88mean prm2s lst2 org2s.t88 >> log2
t88 prm2s lst2 new2s.t88 >> log2
echo ECAT 6 PAIRED >> log2
t88mean prm2p lst2 org2p.t88 >> log2
t88 prm2p lst2 new2p.t88 >> log2

echo ECAT 7 SINGLE > log3
t88mean prm3as lst3 org3s.t88 >> log3
t88 prm3s lst3 new3s.t88 >> log3
echo ECAT 7 PAIRED >> log3
