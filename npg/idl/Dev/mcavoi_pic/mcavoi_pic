#!/bin/csh
#   Name:   mcavoi_pic
#   Author: Tom Videen
#   Date:   Mar 2003
#   Created for: COSS PET test display
#   Intent: Save GIF image of 3 COSS PET images

setenv IDL_NAME mcavoi_pic
setenv RESOURCES vidi.xrdb
setenv SCAN1 p5949ho1_g3.t88
setenv SCAN2 p5949oo1ho1.t88
setenv SCAN3 p5949oo2ho1.t88
setenv VOI   p5949mca.voi
setenv TIF   p5949.tif
run_idl
